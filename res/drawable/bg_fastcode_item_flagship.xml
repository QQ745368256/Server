<?xml version="1.0" encoding="utf-8"?>
<selector
  xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_pressed="false">
        <shape>
            <corners android:radius="12.0dip" />
            <stroke android:width="1.0px" android:color="@color/flagship_item_normal" />
            <solid android:color="@color/flagship_item_normal" />
        </shape>
    </item>
    <item android:state_pressed="true">
        <shape>
            <corners android:radius="12.0dip" />
            <stroke android:width="1.0px" android:color="@color/flagship_item_press" />
            <solid android:color="@color/flagship_item_press" />
        </shape>
    </item>
</selector>